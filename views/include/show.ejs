<% for(var i = 0; i < users.length; i++) { %> <% if (users[i].isAdmin != true )
{ %>
<tr>
  <td><%= i + 1 %></td>
  <td><%= users[i].fullname %></td>
  <td><%= users[i].email %></td>
  <td><%= users[i].telephone %></td>
  <input type="hidden" name="id" value="<%= users[i]._id %>" />
  <td>
    <a
      href="/update-user?id=<%= users[i]._id%>"
      class="btn border-shadow update"
    >
      <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
    </a>
    <a
      href="/delete-user/<%= users[i]._id%>"
      class="btn border-shadow delete"
      data-id="<%= users[i]._id%>"
    >
    <span class="text-gradient"><i class="fas fa-times"></i></span>
    </a>
  </td>
</tr>
<% } %> <% } %>
